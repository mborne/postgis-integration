pipeline {
    agent {
        docker {
            label 'docker'
            image 'mborne/postgis-integration:latest'
            args ' --network devbox '
        }
    }
    stages {
        stage('Import naturalearth') {
            steps {
                sh 'DEBUG=* /opt/postgis-integration/bin/import.js naturalearth'
            }
        }
    }
}